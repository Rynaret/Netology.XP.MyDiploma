@model List<ShopService.Entities.Product>

<h1>Продукты</h1>

<div class="row">
    @foreach (var product in Model)
    {
        <div class="col-xs-4">
            <div class="panel panel-info">
                <div class="panel-heading">
                    <h3 class="panel-title">@product.Name</h3>
                </div>
                <div class="panel-body">
                    Цена: @product.Price $
                </div>
            </div>
        </div>
    }
</div>